public class Dog extends Animal{
  //public Dog(int age){
  public Dog(){   
    System.out.println("In Dog const");
    sound = "bark";
  }
  public void  getAge(){
    System.out.println("In Dog's getAge");
  }
  public void rollOver(){
    System.out.println("In Dog's roll over");
  }
  public String makeSound(){
    return sound;
  }
}