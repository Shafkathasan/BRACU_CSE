import java.util.Scanner;
public class Task11 {
    public static void main(String[] args) { 
        Scanner sc = new Scanner(System.in);
        int[] a = new int[10];
        int i, j;
        for(i=0; i<=a.length-1; ++i) {
            System.out.println("Please Enter Number:");
            for(j=0; j<=i; ++j) {
                if([i]=a[j]) {
                    a[i]=sc.nextInt();
                }
            }
            System.out.println();
        }
    }
}
