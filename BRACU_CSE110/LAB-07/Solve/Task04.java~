public class Task04
{
    public static void main(String[] args)
    { 
        int count=1, sum=0;
        while(count<=600)
        {
            if((count%7==0 && count%9!=0)||(count%7!=0 && count%9==0))
            {
                sum+=count;
            }
            ++count;
        }
        System.out.println(sum);
    }
}
