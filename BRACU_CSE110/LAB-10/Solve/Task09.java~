import java.util.Scanner;
public class Task09 {
    public static void main(String[] args) { 
        Scanner sc = new Scanner(System.in);
        int[] a = new int[15];
        int i;
        for(i=0; i<=a.length-1; ++i) {
            System.out.println("Please Enter Number:");
            a[i]=sc.nextInt();
            if(i>=0 && i<=9){
                System.out.println(a[i]);
            }
        }
    }
}
