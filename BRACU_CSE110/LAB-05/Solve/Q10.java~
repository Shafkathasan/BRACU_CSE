public class Q10 {  
      public static void main(String[] args) {
            int x = 0, y = 0;
            String sum = "0";
            double p;
            while (x < 9) {
                  y = x / 2;
                  while (y < x) {
                        p = (x + 5.0) / 2;
                        sum = (sum + 2) + x + "y * 2" + (int) p ;
                        System.out.println(sum);
                        y = y + 1;
                  }
                  x = x + 2;
                  if (x > 5) {
                        sum = "2";
                  } else {
                        sum += "3";
                  }
            }
      }
}
